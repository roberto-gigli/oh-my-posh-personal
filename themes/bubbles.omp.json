{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "üë§ {{ .UserName }}",
          "foreground": "#E64747",
          "background": "#29315A",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "agnoster_short"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "üìÇ {{ .Path }}",
          "foreground": "#3EC669",
          "background": "#29315A",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_icon": "\uf126 ",
            "cache_duration": "none"
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ .HEAD }}",
          "foreground": "#43CCEA",
          "background": "#29315A",
          "type": "git",
          "style": "diamond"
        },
        {
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}",
          "foreground": "#9B6BDF",
          "background": "#29315A",
          "type": "battery",
          "style": "diamond",
          "properties": {
            "charged_icon": "üîã ",
            "charging_icon": "üîã‚¨Ü ",
            "discharging_icon": "üîã‚¨á "
          },
          "foreground_templates": [
            "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
            "{{if eq \"Discharging\" .State.String}}#ff5722{{end}}",
            "{{if eq \"Full\" .State.String}}#4caf50{{end}}"
          ]
        },
        {
          "type": "shell",
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#5ee7ec",
          "background": "#29315A",
          "template": "{{ .Name }} {{ .Version }}",
          "properties": {
            "macos": "mac"
          }
        },
        {
          "type": "os",
          "style": "diamond",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "#ffffff",
          "background": "#29315A",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}} {{.OS}}",
          "properties": {
            "macos": "mac"
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin",
            "threshold": 150
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "{{ .FormattedMs }}",
          "foreground": "#AEA4BF",
          "background": "#29315A",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "‚ùØ",
          "foreground": "#7FD5EA",
          "background": "#29315A",
          "type": "text",
          "style": "diamond"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}